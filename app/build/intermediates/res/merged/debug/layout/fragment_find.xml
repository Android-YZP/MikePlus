    <RelativeLayout
        android:id="@+id/layout_find_topbar"
	    android:layout_width="match_parent"
	    android:layout_height="@dimen/common_topbar_height"
	    android:background="@color/common_topbar_bg_color" >
	
	    <TextView
	        android:id="@+id/tv_find_topbar_title"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_centerInParent="true"
	        android:text="发现"
	        android:textColor="@color/common_topbar_text_font_color"
	        android:textSize="@dimen/common_topbar_text_font_size"
	        android:textStyle="bold" />
	
	    <Button
	        android:id="@+id/btn_find_search"
	        android:layout_width="@dimen/index_topbar_img_query_width"
	        android:layout_height="match_parent"
	        android:layout_centerVertical="true"
	        android:layout_alignParentRight="true"
	        android:background="@drawable/nav_btn_search" />
	
	</RelativeLayout>
    
    <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="@dimen/find_commit_height"
        android:background="@drawable/find_commit_banckground"
