                android:textColor="@color/find_commit_text_font_color"
                android:textSize="@dimen/find_commit_text_font_size"
                android:layout_marginTop="@dimen/find_commit_text_margin_top"
                />
        </LinearLayout>
        
    </LinearLayout>
    
    
